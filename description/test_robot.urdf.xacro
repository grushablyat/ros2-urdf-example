<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot">

	<xacro:include filename="test_include.xacro"/>

	<link name="world">
	</link>

	<joint name="world_base_joint" type="continuous">
		<origin xyz="1 1 0" rpy="0 0 0"/>
		<parent link="world"/>
		<child link="base"/>
	</joint>

	<link name="base">
		<visual>
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
			<geometry>
				<box size="1 1 0.1"/>
			</geometry>
			<material name="grey"/>
		</visual>
		<collision>
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
			<geometry>
				<box size="1 1 0.1"/>
			</geometry>
		</collision>
		<xacro:inertial_box mass="12" x="0" y="0" z="0">
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
		</xacro:inertial_box>
	</link>

</robot>

